dnf upgrade
dnf install -y cobbler
systemctl start cobblerd
systemctl enable cobblerd
systemctl start httpd
systemctl enable httpd
reboot
//SELinux
sed -i 's/SELINUX=enforcing/SELINUX=disabled/' /etc/selinux/config

//1
